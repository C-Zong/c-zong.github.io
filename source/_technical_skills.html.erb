<ul class="list-unstyled">
  <% data.locales.en[index].skills.each do |skill| %>
    <li class="mb-3">
      <strong class="fs-5"><%= skill.title %>:</strong>
      <ul>
        <% skill.items.each do |item| %>
          <li class="fs-6"><%= item %></li>
        <% end %>
      </ul>
    </li>
  <% end %>
</ul>